{
	"name": "Facemoderator",
	"version": "1.3.3.7",
	"manifest_version": 1,
	"description": "Jus. Jus dont.",
	"run_at": "document_start",
	"content_scripts": [
		{
			"matches": ["https://www.facebook.com/"],
			"js": ["js/script.js"]
		}
